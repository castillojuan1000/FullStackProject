<%- include partials/header.ejs %>
<script type="text/javascript" src="/js/etsy.js"></script>
</head>

<body>
    <button class="btn toggle-btn"><i class="fa fa-filter"></i></button>
    <div class="main-content">
        <div class="filter-section hide-show">
            <div class="container">
                <div class="row top">
                    <div class="col-lg-12">
                        <h2>Search Filter</h2>
                    </div>
                    <div class="col-lg-12 cat-list">

                    </div>
                </div>
                <div class="row" style="padding-top: 10%; padding-bottom: 10%;">
                    <div class="col-lg-12">
                        Price: <br>
                        <br>
                        <span class="init">$ 10</span>
                        <input id="price-slider" type="text" class="span2" value="" data-slider-min="0"
                            data-slider-max="100" data-slider-value="[0,50]" />
                        <span class="final">$ 100</span>
                        <br>
                        <br>
                    </div>
                    <div class="input-group input-group-sm mb-3">
                        <div class="input-group-prepend">
                            <span class="input-group-text" id="min-input">Min</span>
                        </div>
                        <input type="text" class="form-control min-input" aria-label="Small"
                            aria-describedby="inputGroup-sizing-sm">
                        <div class="input-group-prepend">
                            <span class="input-group-text" id="">Max</span>
                        </div>
                        <input type="text" class="form-control max-input" aria-label="Small"
                            aria-describedby="inputGroup-sizing-sm">
                    </div>
                </div>
                <div class="filter item-type">
                    <h3>Item Type</h3>
                    <label class="container check-container" value="0">All Items
                        <input class='box' type="checkbox" checked="checked">
                        <span class="checkmark"></span>
                    </label>
                    <label class="container check-container" value="1">Vintage
                        <input class='box' type="checkbox" checked="checked">
                        <span class="checkmark"></span>
                    </label>
                    <label class="container check-container" value="2">Hand Made
                        <input class='box' type="checkbox" checked="checked">
                        <span class="checkmark"></span>
                    </label>
                </div>
            </div>
        </div>
        <div class="listing-container" id="etsy-images">
        </div>
    </div>
    </div>
</body>

</html>